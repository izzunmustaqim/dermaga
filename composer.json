{
    "name": "myopensoft/dermaga",
    "description": "Dermaga CMS",
    "keywords": ["yii2", "framework", "advanced", "yii2 cms base app"],
    "homepage": "https://myopensoft.github.io/dermaga.web/",
    "type": "project",
    "license": "BSD-3-Clause",
    "support": {
        "issues": "https://github.com/myopensoft/dermaga/issues?state=open",
        "forum": "https://github.com/myopensoft/dermaga/wiki",
        "wiki": "https://github.com/myopensoft/dermaga/wiki",
        "irc": "https://github.com/myopensoft/dermaga/wiki",
        "source": "https://github.com/myopensoft/dermaga"
    },
    "minimum-stability": "stable",
    "require": {
        "php": ">=5.4.0",
        "yiisoft/yii2": ">=2.0.6",
        "yiisoft/yii2-bootstrap": "*",
        "yiisoft/yii2-swiftmailer": "*",
        "dmstr/yii2-adminlte-asset": "2.*",
        "mihaildev/yii2-ckeditor": "*",
        "mihaildev/yii2-elfinder": "*",
        "2amigos/yii2-file-upload-widget": "~1.0",
        "mpdf/mpdf":"*",
        "toriphes/yii2-console-runner": "*"
    },
    "require-dev": {
        "yiisoft/yii2-codeception": "*",
        "yiisoft/yii2-debug": "*",
        "yiisoft/yii2-gii": "*",
        "yiisoft/yii2-faker": "*"
    },
    "config": {
        "process-timeout": 1800
    },
    "extra": {
        "asset-installer-paths": {
            "npm-asset-library": "vendor/npm",
            "bower-asset-library": "vendor/bower"
        }
    }
}
